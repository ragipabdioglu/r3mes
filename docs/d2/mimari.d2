backend: "ğŸš€ BACKEND API\n(FastAPI / Python)" {
  class: backend
  
  # Entry Point
  entry: "Entry Point\nmain.py" {
    class: api
  }
  
  # API Endpoints
  endpoints: "API Endpoints" {
    inference: "/api/inference/*\ninference_endpoints.py" {
      class: api
      generate: "POST /generate"
      stream: "POST /stream"
      models: "GET /models"
    }
    
    serving: "/api/serving/*\nserving_endpoints.py" {
      class: api
      register: "POST /register"
      status: "GET /status"
      heartbeat: "POST /heartbeat"
    }
    
    proposer: "/api/proposer/*\nproposer_endpoints.py" {
      class: api
      submit_gradient: "POST /submit"
      aggregation: "GET /aggregation"
    }
    
    validator: "/api/validators/*\nvalidator_endpoints.py" {
      class: api
      trust_scores: "GET /trust-scores"
      validate: "POST /validate"
    }
    
    miner: "/api/miners/*\nminer_endpoints.py" {
      class: api
      stats: "GET /stats"
      leaderboard: "GET /leaderboard"
    }
    
    faucet: "/api/faucet/*\nfaucet.py" {
      class: api
      request: "POST /request"
      balance: "GET /balance"
    }
    
    auth: "/api/auth/*\nauth_endpoints.py" {
      class: api
      login: "POST /login"
      register: "POST /register"
      refresh: "POST /refresh"
    }
    
    dashboard: "/api/dashboard/*\ndashboard_endpoints.py" {
      class: api
      network_stats: "GET /network"
      overview: "GET /overview"
    }
    
    analytics: "/api/analytics/*\nanalytics_endpoints.py" {
      class: api
      usage: "GET /usage"
      metrics: "GET /metrics"
    }
    
    websocket: "/ws/*\nwebsocket_endpoints.py" {
      class: api
      realtime: "WS /realtime"
      logs: "WS /logs"
    }
    
    health: "/health/*\nhealth_endpoints.py" {
      class: api
      liveness: "GET /live"
      readiness: "GET /ready"
    }
    
    system: "/system/*\nsystem_endpoints.py" {
      class: api
      version: "GET /version"
      time: "GET /time"
    }
  }
  
  # Services Layer
  services: "Service Layer" {
    model_manager: "AIModelManager\nmodel_manager.py" {
      class: module
    }
    semantic_router: "SemanticRouter\nsemantic_router.py" {
      class: module
    }
    task_queue: "TaskQueue\ntask_queue.py" {
      class: module
    }
    inference_executor: "InferenceExecutor\ninference_executor.py" {
      class: module
    }
    multi_gpu: "MultiGPUManager\nmulti_gpu_manager.py" {
      class: module
    }
  }
  
  # Blockchain Integration
  blockchain_integration: "Blockchain Integration" {
    rpc_client: "BlockchainRPCClient\nblockchain_rpc_client.py" {
      class: module
    }
    query_client: "BlockchainQueryClient\nblockchain_query_client.py" {
      class: module
    }
    event_listener: "EventListener\nblockchain_event_listener.py" {
      class: module
    }
    adapter_sync: "AdapterSyncService\nadapter_sync_service.py" {
      class: module
    }
    indexer: "Indexer\nindexer.py" {
      class: module
    }
  }
  
  # Security Layer
  security: "Security Layer" {
    auth: "Auth\nauth.py"
    jwt: "JWT\njwt_auth.py"
    sanitizer: "InputSanitizer\ninput_sanitizer.py"
    validator: "InputValidator\ninput_validation.py"
    secrets: "SecretsProvider\nsecrets_provider.py"
    vault: "VaultClient\nvault_client.py"
  }
  
  # Middleware
  middleware: "Middleware" {
    error_handler: "ErrorHandler\nerror_handlers.py"
    rate_limit: "RateLimit\n(slowapi)"
    cors: "CORS"
    cache: "CacheMiddleware\ncache_middleware.py"
    trace: "TraceMiddleware\ntrace_middleware.py"
    debug: "DebugMiddleware\ndebug_middleware.py"
  }
  
  # Data Layer
  data: "Data Layer" {
    database: "Database\ndatabase_async.py" {
      class: storage
    }
    postgres: "PostgreSQL\ndatabase_postgres.py" {
      class: storage
    }
    cache: "Redis Cache\ncache.py" {
      class: storage
    }
    repositories: "Repositories" {
      base: "BaseRepository"
      user: "UserRepository"
      api_key: "APIKeyRepository"
    }
  }
  
  # Monitoring
  monitoring: "Monitoring" {
    metrics: "Prometheus\nmetrics.py"
    health: "HealthMetrics\nhealth_metrics.py"
    system: "SystemMetrics\nsystem_metrics_collector.py"
    sentry: "Sentry\nsentry.py"
    otel: "OpenTelemetry\nopentelemetry_setup.py"
    audit: "AuditLogger\naudit_logger.py"
  }
}

# Backend Internal Connections
backend.entry -> backend.endpoints: "routes"
backend.entry -> backend.middleware: "applies"
backend.endpoints.inference -> backend.services.model_manager: "uses"
backend.endpoints.inference -> backend.services.semantic_router: "routes"
backend.endpoints.inference -> backend.services.task_queue: "queues"
backend.services.model_manager -> backend.services.inference_executor: "executes"
backend.services.inference_executor -> backend.services.multi_gpu: "manages GPUs"

backend.blockchain_integration.event_listener -> backend.blockchain_integration.adapter_sync: "triggers sync"
backend.blockchain_integration.adapter_sync -> backend.services.model_manager: "updates adapters"
backend.blockchain_integration.query_client -> backend.blockchain_integration.rpc_client: "uses"

backend.endpoints -> backend.security: "protected by"
backend.services -> backend.data: "persists to"
backend.data.repositories -> backend.data.database: "queries"
backend.data.database -> backend.data.postgres: "connects"

